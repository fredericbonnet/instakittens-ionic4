{
  "name": "instakittens-ionic4",
  "version": "0.0.1",
  "description": "Ionic 4/Angular application for the Instakittens project",
  "author": {
    "name": "Frédéric Bonnet",
    "email": "fredericbonnet@free.fr"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fredericbonnet/instakittens-ionic4.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:watch-no-serve": "nodemon -e js,ts -x npx ng e2e --devServerTarget= --webdriverUpdate=false",
    "e2e:cucumber": "ng e2e --protractorConfig=e2e/protractor.cucumber.conf.js",
    "e2e:cucumber:watch-no-serve": "nodemon -e js,ts,feature -x npx ng e2e --protractorConfig=e2e/protractor.cucumber.conf.js --devServerTarget= --webdriverUpdate=false",
    "cypress:run": "cypress run",
    "cypress:watch": "nodemon -e js,ts -x npm run cypress:run",
    "cypress:open": "cypress open",
    "codeceptjs": "codeceptjs run --config codeceptjs/codecept.conf.js",
    "codeceptjs:watch": "nodemon -e js,ts,feature -x npm run codeceptjs",
    "testcafe": "node testcafe",
    "testcafe:watch": "node testcafe --live",
    "puppeteer": "mocha --opts puppeteer/mocha.opts --exit",
    "puppeteer:watch": "mocha --opts puppeteer/mocha.opts -w",
    "cucumber": "cd puppeteer && cucumber-js",
    "cucumber:watch": "nodemon -e js,feature -x npm run cucumber -- -- --profile watch",
    "playwright": "mocha --opts playwright/mocha.opts --exit",
    "playwright:watch": "mocha --opts playwright/mocha.opts -w",
    "cucumber-playwright": "cd playwright && cucumber-js",
    "cucumber-playwright:watch": "nodemon -e js,feature -x npm run cucumber-playwright -- -- --profile watch"
  },
  "dependencies": {
    "@angular/common": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^4.11.5",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.803.19",
    "@angular-devkit/build-angular": "~0.803.19",
    "@angular-devkit/core": "~8.3.19",
    "@angular-devkit/schematics": "~8.3.19",
    "@angular/cli": "~8.3.19",
    "@angular/compiler": "~8.2.14",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@ionic/angular-toolkit": "~2.1.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.0",
    "chai": "^4.2.0",
    "codeceptjs": "^2.3.6",
    "codelyzer": "^5.0.1",
    "cucumber": "^6.0.5",
    "cypress": "^3.6.1",
    "cypress-cucumber-preprocessor": "^1.16.2",
    "faker": "^4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.1",
    "playwright": "^0.9.21",
    "protractor": "~5.4.2",
    "protractor-cucumber-framework": "^6.2.0",
    "puppeteer": "^1.20.0",
    "testcafe": "^1.7.0",
    "ts-node": "~8.0.0",
    "tslint": "~5.12.0",
    "typescript": "~3.5.3"
  }
}
